{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install -r dashboard/requirements.txt"
  },
  "deploy": {
    "startCommand": "cd dashboard && gunicorn app:server --bind 0.0.0.0:$PORT --workers 2",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  }
}